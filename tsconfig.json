{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2018",
    "sourceMap": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./packages",
    "paths": {
      "@alphaapps/nestjs-db": ["nestjs-db/src"],
      "@alphaapps/nestjs-db/*": ["nestjs-db/src/*"],
      "@alphaapps/nestjs-config": ["nestjs-config/src"],
      "@alphaapps/nestjs-config/*": ["nestjs-config/src/*"],
      "@alphaapps/nestjs-common": ["nestjs-common/src"],
      "@alphaapps/nestjs-common/*": ["nestjs-common/src/*"],
      "@alphaapps/nestjs-auth": ["nestjs-auth/src"],
      "@alphaapps/nestjs-auth/*": ["nestjs-auth/src/*"],
      "@alphaapps/nestjs-logs": ["nestjs-logs/src"],
      "@alphaapps/nestjs-logs/*": ["nestjs-logs/src/*"]
  }
},
  "references": [
    { "path": "nestjs-db"},
    { "path": "nsetjs-config" },
    { "path": "nestjs-common"},
    { "path": "nestjs-auth"},
    { "path": "nestjs-logs"}
  ]
}
